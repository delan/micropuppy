{
    "editor.formatOnSave": true,
    "rust-analyzer.cargo.features": "all",
    "rust-analyzer.cargo.buildScripts.overrideCommand": [
        "make",
        "-s",
        "lsp-check",
    ],
    // most other rust-analyzer.check.* settings will be ignored
    "rust-analyzer.check.overrideCommand": [
        "make",
        "-s",
        "lsp-check",
    ],
}
